//  Student.h
#import<Foundation/Foundation.h>

NS_ASSUME_NONNULL_BEGIN

@interface Student ï¼šNSObject

@property (nonatomic,copy) NSString *name;

@property (nonatomic,copy) NSString *major;

@property (nonatomic,assign) NSUinteger age;

-(void)study;

-(float)time;
